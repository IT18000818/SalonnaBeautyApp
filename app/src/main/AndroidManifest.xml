    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".JoinNow"></activity>
        <activity android:name=".ViewAppointments" />
        <activity android:name=".AppointmentDetails" />
        <activity android:name=".Appointment" />
        <activity android:name=".Beautician" />
        <activity android:name=".Bridal" />
        <activity android:name=".GentsCategory" />
        <activity android:name=".LadiesCategory" />
        <activity android:name=".genderselection" />
        <activity android:name=".StartingPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>